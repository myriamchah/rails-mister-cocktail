<h1>In a mood for a new cocktail ?</h1>

<h2 class="p-3">Please, add a new one: </h2>

<div class="p-3">
<%= simple_form_for @cocktail do |f| %>
  <%= f.input :name  %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit "Submit", class: "btn btn-danger d-block" %>
<% end %>
</div>

